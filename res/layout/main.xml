<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res/com.murdersquirrel.newboard"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@raw/trinidad"
    android:orientation="vertical" >

    <Button
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/button01"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="29dp"
        android:background="@drawable/buttonbg"
        android:gravity="left|center"
        android:padding="10px"
        android:shadowColor="#f5f5f5"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Molly"
        android:textColor="#6c6c6c"
        android:textSize="6pt" />

    <Button
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/button02"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button01"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/button01"
        android:layout_marginRight="60dp"
        android:background="@drawable/buttonbg"
        android:gravity="left|center"
        android:padding="10px"
        android:shadowColor="#f5f5f5"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Just Watch"
        android:textColor="#6c6c6c"
        android:textSize="6pt"
        android:typeface="normal" />

    <Button
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/button03"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button01"
        android:layout_centerVertical="true"
        android:background="@drawable/buttonbg"
        android:gravity="left|center"
        android:padding="10px"
        android:shadowColor="#f5f5f5"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Freshmen"
        android:textColor="#6c6c6c"
        android:textSize="6pt" />

    <Button
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/button04"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button03"
        android:layout_alignBottom="@+id/button03"
        android:layout_alignLeft="@+id/button02"
        android:background="@drawable/buttonbg"
        android:gravity="left|center"
        android:padding="10px"
        android:shadowColor="#f5f5f5"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Snitches"
        android:textColor="#6c6c6c"
        android:textSize="6pt" />

    <Button
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/button05"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button03"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="69dp"
        android:background="@drawable/buttonbg"
        android:gravity="left|center"
        android:padding="10px"
        android:shadowColor="#f5f5f5"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Spellmen"
        android:textColor="#6c6c6c"
        android:textSize="6pt" />

</RelativeLayout>
